<div class="item-main">


<div id="container" class="">
<%@products.each do |product|%>
    <div class="item" style="">

      <div class="item__img-wrap item__img-wrap_choice">
        <%if product.variants.length > 1%>
        <div class="color-choice">
          <ul class="color-choice__list">
            <%product.variants.each do |var|%>
            <li class="color-choice__item">
              <a class="color-choice__link" style="color: <%=var.color.code%>" href="#"><i class="fa fa-circle"></i></a>
            </li>
            <% end%>
          </ul>
        </div>
        <%end%>

        <a class="item__img-link item__img-link_bs" href="#">
          <img data-lazy-src=<%= image_url product.main_variant.pictures[0].image%> alt="Rhythmic Gymnastics Leotard #29"/>
          <div class="item__name-link-wrap">
            <p class="item__name-link">Rhythmic Gymnastics Leotard #29</p>
          </div>
         <%if product.stock_for_sale!='' %>
          <div class="item__stock-img"></div>
         <%end%>
        </a>
      </div>

      <div class="item__bottom">
        <%if product.old_price > product.price%>
            <div class="item__price item__price_strike"><i class="fa fa-eur"></i><%=product.old_price%>.-</div>
            <div class="item__price item__price_red"><i class="fa fa-eur"> </i><%=product.price%>.-</div>
            <%else%>
            <div class="item__price"><i class="fa fa-eur"> </i>109.-</div>
            <%end%>
        <a class="item__rotator" href="#"></a>
        <a class="item__like-off" href="#"></a>
      </div>

    </div>


<% product.variants.each do |variant|%>
<div class="item" style="">

  <div class="item__img-wrap">
    <a class="item__img-link item__img-link_bs" href="#">
      <img data-lazy-src=<%=image_url variant.pictures[0].image%> alt="Rhythmic Gymnastics Leotard #29"/>
      <div class="item__name-link-wrap">
        <p class="item__name-link">Rhythmic Gymnastics Leotard #29</p>
      </div>
    </a>
  </div>

  <div class="item__bottom">
    <%if variant.product.old_price > variant.product.price%>
        <div class="item__price item__price_strike"><i class="fa fa-eur"></i><%=product.old_price%>.-</div>
        <div class="item__price item__price_red"><i class="fa fa-eur"> </i><%=product.price%>.-</div>
    <%else%>
        <div class="item__price"><i class="fa fa-eur"> </i>109.-</div>
    <%end%>
    <a class="item__rotator" href="#"></a>
    <a class="item__like-off" href="#"></a>
  </div>

</div>
<%end; end%>

